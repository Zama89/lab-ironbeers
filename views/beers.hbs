<div class='container-beers'>
    {{#each beers}}
            <img src='{{this.image_url}}' class='img-beers' alt='{{this.name}}' />
            <div class='beers-body'>
                <h5 class='beers-title'>{{this.name}}</h5>
                <p class='beers-tagline'>{{this.tagline}}</p>
                <p class='beers-text'>{{this.description}}</p>
            </div> 
            {{> beerPartial }}
    {{/each}}
</div>